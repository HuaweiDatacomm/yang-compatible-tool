<?xml version="1.0" encoding="UTF-8"?>
<white-box-filter tag = "tag-name">
	<common-filter>
		<filename-filter>
			<reg-match>
				<match>huawei-aaa*</match>
				<match>openconfig*</match>
				<match>ietf-*</match>
			</reg-match>
		</filename-filter>
		<errorinfo-filter>
			<reg-match>
				<!-- 如果 * 在第一个，需要加 . 进行转义 -->
				<match>.*must have a "description" substatement*</match>
				<match>.*must have a 'description' substatement*</match>
				<match>.*must have a 'reference' substatement*</match>
				<match>.*must have a "reference" substatement*</match>
			</reg-match>
		</errorinfo-filter>
	</common-filter>
